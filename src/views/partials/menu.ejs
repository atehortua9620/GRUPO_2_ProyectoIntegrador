<header>
    
    <a class='linkmenus' href="/"><img class='logoMenu' src="/images/Logo_negro.png" alt="logo"></a>
    
    <form class="contenedorUno" >
        <h1 class="titulo">Swiss house of watches</h1>
        <div class="barraYlupa"><input class="search letra"  type="text"><i class="fas fa-search"></i></div>
        <div class="menus">
            <select name="Categorias" class="barraYlupa letra">
                <option value="Categories">Categories</option>
                <option value="Classics">Classics</option>
                <option value="Smart">Smart</option>
                <option value="Sport">Sport</option>
                <option value="Man">Mens</option>
                <option value="Womans">Womans</option>
                
            </select>
            
        </div>
        <!-- <button class="search" type="submit">Search</button> -->
        
    </form>
    <div class=" icons">
        
        <a href="/"><i class="icon fas fa-home"></i></a>
        <a href="/productCar"><i class="icon fas fa-cart-arrow-down"></i></a>
        <%if(userlogged == undefined){%>
        <a href="/users/register"><i class="icon fas fa-user-circle"></i></a>
        <%}else{%>
            <a id= 'logout'href='/logout' class="icon fa-solid fa-right-from-bracket"></a>
            <%if(userlogged.roll == 'super'){%>
            <a href="/product/createEditProduct" class="icon fa-solid fa-screwdriver"></a>
            <%}%>
            <a href="/users/register"><img class="avatarImagen" src="/images/users/<%=userlogged.imagen%>" alt="usuario"></a>
            
            <%}%>
    </div>
    
    <label for="burguer">
        <input class="burguer" type="checkbox" id="burguer"/>
        <i class=" burguer icon fas fa-bars"></i>
    </label>

    <script src="js/killSessionHandler.js"></script>
</header>